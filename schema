CREATE VIRTUAL TABLE gifsearch USING fts3(id, rating, tags, title);

CREATE TABLE gifs (
  id TEXT,
  username TEXT,
  type TEXT,
  slug TEXT,
  url TEXT,
  bitly_url TEXT,
  embed_url TEXT,
  source TEXT,
  rating TEXT,
  source_tld TEXT,
  source_post_url TEXT,
  update_timestamp TEXT,
  create_datetime TEXT,
  import_datetime TEXT,
  trending_datetime TEXT,
  title TEXT,
);
